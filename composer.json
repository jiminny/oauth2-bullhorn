{
    "name": "jiminny/oauth2-bullhorn",
    "description": "Bullhorn OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Marinov",
            "email": "alex.g.marinov@gmail.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "bullhorn"
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^2.18"
    },
    "autoload": {
        "psr-4": {
            "Jiminny\\OAuth2\\Client\\": "src/"
        }
